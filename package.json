{
  "name": "nubrid",
  "description": "Nubrid",
  "version": "0.0.1",
  "private": true,
  "author": "Randell Rivera",
  "scripts": {
    "// ============================== CLI ============================== ": "",
    "// ============================== BATCH JOBS ============================== ": "",
    "// ============================== SERVERS ============================== ": "",
    "// ============================== SAPPER (20201017) ============================== ": "",
    "dev": "sapper dev",
    "build": "sapper build",
    "export": "sapper export",
    "start": "node __sapper__/build"
  },
  "// ============================== BROWSERSLIST ============================== ": "",
  "browserslist": {
    "production": [
      "defaults or > 0.2% in my stats and supports es6 and not IE 11 and not Op_Mini all and not baidu < 7.13 and not kaios < 2.6",
      "last 2 Chrome major versions",
      "last 2 Edge major versions",
      "last 2 Firefox major versions",
      "last 2 Opera major versions",
      "last 2 Safari major versions"
    ],
    "modern": [
      "defaults or > 0.2% in my stats and supports es6 and not IE 11 and not Op_Mini all and not baidu < 7.13 and not kaios < 2.6",
      "last 2 Chrome versions",
      "last 2 Edge versions",
      "last 2 Firefox versions",
      "last 2 Opera versions",
      "last 2 Safari versions"
    ],
    "ssr": [
      "maintained node versions"
    ]
  },
  "// TODO: Revisit parser, rules ============================== ESLINT ============================== ": "",
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "es2017": true,
      "es2020": true,
      "es2021": true,
      "node": true,
      "serviceworker": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "globalReturn": true
      },
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "airbnb-base",
      "plugin:sonarjs/recommended",
      "plugin:unicorn/recommended",
      "prettier"
    ],
    "plugins": [
      "immutable",
      "sonarjs",
      "svelte3",
      "unicorn"
    ],
    "overrides": [
      {
        "files": [
          "*.svelte"
        ],
        "processor": "svelte3/svelte3"
      }
    ],
    "rules": {
      "immutable/no-let": "warn",
      "immutable/no-this": "warn",
      "immutable/no-mutation": "warn",
      "import/prefer-default-export": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "import/no-mutable-exports": "off",
      "block-scoped-var": "error",
      "complexity": [
        "error",
        10
      ],
      "dot-notation": "error",
      "eqeqeq": "error",
      "global-require": "off",
      "guard-for-in": "error",
      "linebreak-style": [
        "error",
        "windows"
      ],
      "max-depth": [
        "error",
        5
      ],
      "max-lines": [
        "error",
        100
      ],
      "max-lines-per-function": [
        "error",
        {
          "max": 20,
          "skipBlankLines": true,
          "skipComments": true
        }
      ],
      "max-nested-callbacks": [
        "error",
        3
      ],
      "max-params": [
        "error",
        5
      ],
      "max-statements": [
        "error",
        10
      ],
      "max-statements-per-line": "error",
      "new-cap": "error",
      "no-bitwise": "error",
      "no-caller": "error",
      "no-eval": "error",
      "no-extend-native": "error",
      "no-iterator": "error",
      "no-loop-func": "error",
      "no-multi-str": "error",
      "no-new": "error",
      "no-param-reassign": "error",
      "no-plusplus": "error",
      "no-proto": "error",
      "no-script-url": "error",
      "no-shadow": [
        "error",
        {
          "hoist": "all",
          "allow": [
            "event"
          ]
        }
      ],
      "no-undef": "error",
      "no-use-before-define": "error",
      "no-var": "error",
      "strict": [
        "error",
        "never"
      ],
      "unicorn/prevent-abbreviations": [
        "error",
        {
          "replacements": {
            "dev": false,
            "pkg": false
          }
        }
      ],
      "wrap-iife": "error"
    },
    "globals": {}
  },
  "// ============================== PRETTIER (Reload VS Code after change) ============================== ": "",
  "prettier": {
    "semi": false,
    "svelteBracketNewLine": true,
    "svelteIndentScriptAndStyle": false,
    "svelteSortOrder": "scripts-markup-styles",
    "trailingComma": "all"
  },
  "// ============================== SAPPER (20201017) ============================== ": "compression, polka, sirv",
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "// ============================== SAPPER (20201017 - DEV) ============================== ": "file-loader, sapper, svelte, svelte-loader, webpack, webpack-modules",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "autoprefixer": "^10.0.4",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "eslint-plugin-unicorn": "^23.0.0",
    "file-loader": "^6.0.0",
    "postcss": "^8.1.14",
    "postcss-load-config": "^3.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-tailwind": "^2.2.6",
    "sapper": "^0.28.0",
    "sass": "^1.30.0",
    "svelte": "^3.17.3",
    "svelte-loader": "^2.9.0",
    "svelte-preprocess": "^4.6.1",
    "svelte-prettier-plugin": "npm:prettier-plugin-svelte@^1.4.1",
    "tailwindcss": "^2.0.1",
    "webpack": "^4.7.0",
    "webpack-modules": "^1.0.0"
  }
}
